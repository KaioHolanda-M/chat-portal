const express = require('express');
const app = express();

app.use((req, res, next) => {
  res.setHeader('X-Frame-Options', 'ALLOW-FROM https://brbseguros.sharepoint.com/sites/Teste0905/SitePages/TrainingHome.aspx');
  next();
});

app.use(express.static('public')); 

const port = 3000;
app.listen(port, () => {
  console.log(`Servidor rodando na porta ${port}`);
});
